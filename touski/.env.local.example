# Pause CJ (do not remove code)
CJ_ENABLED=0

# EPROLO config
# Required: API key (do NOT commit real secrets)
EPROLO_API_KEY=
EPROLO_EMAIL=

# Optional: direct URL if your account requires a specific endpoint
# Defaults to https://openapi.eprolo.com
# EPROLO_SEARCH_URL=

# Optional: extra params merged into request body/query (JSON)
# Example: {"language":"EN"}
# EPROLO_EXTRA=

# Enable mock data for demo without hitting network
# EPROLO_MOCK=1

# Optional filtering keywords
# EPROLO_HOME_ALLOW=["home","kitchen", "bath", "lighting", "furniture", "storage", "garden", "bedding", "clean"]
# EPROLO_HOME_BLOCK=["clothing","jeans","dress","jewelry","bag","shoes","wallet"]
